<div id="app">
	<div id="header">
		<h1>Yarda <spam id="yard">@ViewBag.yard.Yard1</spam></h1>
	</div>
	<div id="content">
		<div class="box">
			<div class="table">
				<div>
					<div></div>
					<div><h2 class="center">Actual</h2></div>
					<div><h2 class="center">Max</h2></div>
				</div>
				<div>
					<div><h2>Equipos</h2></div>
					<div><h1 class="center"><spam id="max_equipment">@ViewBag.yard.cur_equipment</spam></h1></div>
					<div><h1 class="center"><spam id="cur_equipment">@ViewBag.yard.max_equipment</spam></h1></div>
				</div>
				<div>
					<div><h2>Unidades</h2></div>
					<div><h1 class="center"><spam id="max_units">@ViewBag.yard.cur_units</spam></h1></div>
					<div><h1 class="center"><spam id="cur_units">@ViewBag.yard.max_units</spam></h1></div>
				</div>
			</div>
		</div>
	</div>
	<div id="footer">
		<div class="table">
			<div>
				<div><h3 class="center">Última Actualización</h3></div>
				<div><h3 class="center">Fecha actual</h3></div>
			</div>
			<div>
				<div><h3 class="center"><spam id="update">@ViewBag.yard.update.ToString("MM/dd/yyyy HH:mm:ss")</spam></h3></div>
				<div>
					<h3 id="clock" class="center"></h3>
				</div>
			</div>
		</div>
	</div>
</div>